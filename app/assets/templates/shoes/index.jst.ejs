<div class="row">

     <%  shoes.each(function(shoe) { %>
       <a href="#/shoes/<%= shoe.get("id") %>" class="col-xs-4 centered-item">
       <div class="image-index">
         <h4><%= shoe.escape("name") %><br/></h4>
         <h4 class="matches">
           <%= shoe.likedBy().length %> likes,
           <%= shoe.matchingItems().length %> mixes,
           <%= shoe.comments().length %> comments
         </h4>

           <img src="<%= shoe.escape("shoe_photo_md") %>" class="img-responsive index inline-image"/>

       </div>
        </a>
     <% }); %>

</div>



<div id="page" class="Kicks Reviews"></div>